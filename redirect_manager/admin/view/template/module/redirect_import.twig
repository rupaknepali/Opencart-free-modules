{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="float-end">
        <button type="submit" form="form-redirect-import" data-bs-toggle="tooltip" title="{{ button_import }}" class="btn btn-primary"><i class="fa-solid fa-upload"></i></button>
        <a href="{{ back }}" data-bs-toggle="tooltip" title="{{ button_back }}" class="btn btn-light"><i class="fa-solid fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ol class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
          <li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ol>
    </div>
  </div>
  <div class="container-fluid">
    <div class="card">
      <div class="card-header"><i class="fa-solid fa-pencil"></i> {{ text_import }}</div>
      <div class="card-body">
        <form id="form-redirect-import" action="{{ action }}" method="post" enctype="multipart/form-data">
          <div class="row mb-3">
            <label for="input-file" class="col-sm-2 col-form-label">{{ entry_file }}</label>
            <div class="col-sm-10">
              <input type="file" name="import" id="input-file" class="form-control"/>
              <div class="form-text">{{ help_file }}</div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
{{ footer }}
